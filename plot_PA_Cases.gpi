set terminal png size 900,900
set output 'cases.png'
set multiplot layout 2, 1 title "PA COVID-19 Cases"
set title "Standard Scale"
set xlabel "Date"
set xdata time
set timefmt "%m/%d/%y"
set ylabel "Number of Confirmed Cases"
set grid
unset key
plot "cases.data" using 1:2 with linespoints linestyle 1 title "Cases"
#
set logscale y
unset key
set title "Log Scale"
plot "cases.data" using 1:2 with linespoints linestyle 1 title "Cases"
#
unset multiplot
